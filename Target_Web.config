      <rules>
        <rule name="DynamicParameters1">
          <match url="(.*)\$\$.*\$\$(.*)"/>
          <action type="Rewrite" url="/Superviseur/DynamicParameters/DynamicParameters?url={R:0}" appendQueryString="true"/>
          <conditions>
            <add input="{REQUEST_FILENAME}" pattern="DynamicParameters" negate="true"/>
          </conditions>
        </rule>
        <rule name="DynamicParameters2">
          <match url="(.*)"/>
          <action type="Rewrite" url="/Superviseur/DynamicParameters/DynamicParameters?url={R:0}" appendQueryString="true"/>
          <conditions>
            <add input="{QUERY_STRING}" pattern="(.*)\$\$.*\$\$(.*)"/>
            <add input="{REQUEST_FILENAME}" pattern="DynamicParameters" negate="true"/>
          </conditions>
        </rule>
        <rule name="DynamicParameters3">
          <match url="(.*)RegisterTrainingAndRedirect\/(.*)"/>
          <action type="Rewrite" url="/Supervision/DynamicParameters/RegisterTrainingSelfServiceAndRedirect/{R:2}" appendQueryString="false"/>
        </rule>
        <rule name="DynamicParametersGuestForm" stopProcessing="true">
          <match url="(.*)GuestAction\/(.*)\/(\d+)"/>
          <action type="Redirect" url="/Supervision/DynamicParameters/GuestForm?actionType={R:2}&amp;actionId={R:3}" appendQueryString="true"/>
        </rule>
        <rule name="DynamicParameters4">
          <match url="(.*)RegisterConversation\/(.*)"/>
          <action type="Rewrite" url="/Supervision/DynamicParameters/AddToConversationAndRedirect/{R:2}" appendQueryString="false"/>
        </rule>
        <rule name="Spa application react-router" stopProcessing="true">
          <match url="spa/(.*)"/>
          <conditions>
            <add input="{HTTP_METHOD}" pattern="^GET$"/>
            <add input="{HTTP_ACCEPT}" pattern="^text/html"/>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true"/>
          </conditions>
          <action type="Rewrite" url="/spa/index.html"/>
        </rule>
      </rules>
    </rewrite>
