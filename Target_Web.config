        <add name="Access-Control-Allow-Origin" value="https://deploiementsuite.syfadis.com"/>
        <add name="Access-Control-Allow-Methods" value="*"/>
        <add name="Access-Control-Allow-Headers" value="*"/>
        <add name="Access-Control-Allow-Credentials" value="true"/>
        <add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains"/>
        <add name="X-Frame-Options" value="SAMEORIGIN"/>
        <add name="X-Xss-Protection" value="1; mode=block"/>
        <add name="X-Content-Type-Options" value="nosniff"/>
        <add name="Content-Security-Policy"
          value="default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; img-src * 'self' data: https: blob: ;font-src * 'self' data: https:;worker-src 'self' data: https: blob:; media-src * data: https: blob:;"/>
        <add name="Referrer-Policy" value="strict-origin-when-cross-origin"/>
        <add name="feature-policy" value="camera 'none', interest-cohort 'none', payment 'none'"/>
        <add name="permissions-policy" value="camera=(), interest-cohort=(), payment=()"/>
      </customHeaders>
    </httpProtocol>
