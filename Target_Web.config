  <HttpAntiLeechModuleSection>
    <AccessDeniedUrl>
      <RegExpression value="/Files/ForcedPlayer/"/>
      <RegExpression value="/Files/Scripts/[^(css)]"/>
      <RegExpression value="/Files/Application/[0-9]+/Documents/"/>
      <RegExpression value="/Files/Application/[0-9]+/EditorResources/"/>
      <RegExpression value="/Files/Application/[0-9]+/FtpPackages/"/>
      <RegExpression value="/Files/Application/[0-9]+/Import/"/>
      <RegExpression value="/Files/Application/[0-9]+/Licence/"/>
      <RegExpression value="/Files/Application/[0-9]+/Packages/"/>
      <RegExpression value="/Files/Application/[0-9]+/Pictures/"/>
      <RegExpression value="/Files/Application/[0-9]+/Reports/"/>
      <RegExpression value="/Files/Application/[0-9]+/Traces/"/>
      <RegExpression value="/Files/Application/[0-9]+/Wikis/"/>
    </AccessDeniedUrl>
    <AllowedUrlReferer>
      <RegExpression value=".*.ca-ifcam.fr/"/>
    </AllowedUrlReferer>
  </HttpAntiLeechModuleSection>
  <telerik.web.ui>
    <!-- #25828 - RadCompression is deprectaed - this will be removed at a future date -->
